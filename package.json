{
  "name": "@technik-sde/renovate-config",
  "version": "0.0.0",
  "description": "Renovate configuration for all repositories of the online-block team",
  "license": "UNLICENSED",
  "renovate-config": {
    "default": {
      "extends": [
        "config:base",
        "group:allNonMajor",
        "schedule:nonOfficeHours",
        ":automergeMinor",
        ":automergeRequireAllStatusChecks"
      ],
      "commitMessagePrefix": "chore: [{{{prettyDepType}}}]",
      "commitMessageExtra": "from v{{{currentVersion}}} to {{#if isMajor}}v{{{newMajor}}}{{else}}{{#if isSingleVersion}}v{{{newVersion}}}{{else}}{{{newValue}}}{{/if}}{{/if}}",
      "encrypted": {
        "npmToken": ""
      }
    }
  }
}
